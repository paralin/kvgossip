{
  "name": "@fusebot/kvgossip",
  "description": "JavaScript client bindings for KVGossip.",
  "main": "js/lib/index.js",
  "typings": "js/lib/index.d.ts",
  "license": "UNLICENSED",
  "dependencies": {
    "protobufjs": "^6.0.0"
  },
  "scripts": {
    "build": "cd js && tsc",
    "lint": "cd js && tslint -c tslint.json --project tsconfig.json --type-check",
    "test": "npm run lint && npm run mocha",
    "gen-proto": "./scripts/gen_proto.bash",
    "ci": "npm run build && npm run test",
    "mocha": "ts-node ./node_modules/istanbul/lib/cli.js cover -e .ts  -x \"*.d.ts\" -x \"*.spec.ts\" ./js/test/run_tests.js",
    "mocha-nocover": "ts-node ./js/test/run_tests.js",
    "postpublish": "greenkeeper-postpublish"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/jasmine": "^2.5.38",
    "@types/long": "^3.0.31",
    "@types/node": "^6.0.46",
    "babel-core": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "cz-conventional-changelog": "^1.2.0",
    "greenkeeper-postpublish": "^1.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine": "^2.5.0",
    "jasmine-console-reporter": "^1.2.0",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.2",
    "typescript": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FuseRobotics/kvgossip.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
